{% schema %}
{
  "name": "Temperature Retention",
  "tag": "section",
  "class": "temperature-retention-section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Título Principal",
      "default": "SUPERIOR TEMPERATURE RETENTION"
    },
    {
      "type": "textarea",
      "id": "paragraph",
      "label": "Texto descriptivo",
      "default": "Keep your coffee hot. Keep your water cold.<br>Keep your food fresh and your day under control."
    },
    {
      "type": "image_picker",
      "id": "cold_image",
      "label": "Imagen Cold"
    },
    {
      "type": "text",
      "id": "cold_text1",
      "label": "Cold: línea 1",
      "default": "Keeps"
    },
    {
      "type": "text",
      "id": "cold_text2",
      "label": "Cold: línea 2",
      "default": "cold"
    },
    {
      "type": "image_picker",
      "id": "hot_image",
      "label": "Imagen Hot"
    },
    {
      "type": "text",
      "id": "hot_text1",
      "label": "Hot: línea 1",
      "default": "Keeps"
    },
    {
      "type": "text",
      "id": "hot_text2",
      "label": "Hot: línea 2",
      "default": "Hot"
    }
  ],
  "presets": [
    {
      "name": "Temperature Retention",
      "category": "Custom"
    }
  ]
}
{% endschema %}

